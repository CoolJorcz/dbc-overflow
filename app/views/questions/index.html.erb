<div>
  <span>
    <% unless session[:user_id] %>
      <%=  link_to  "Sign In", new_session_path %> <%= link_to "Sign Up", new_user_path %></span>
    <%  else %>
      <%= link_to "Log Out", logout_path %>
    <% end %>
    <h2>Questions</h2>

  <ul>
    <% @questions.each do |question| %>
    <li>
      <%=  link_to  question.title, question_path(question) %>
    </li>
    <% end %>
  </ul>
</div>

   
